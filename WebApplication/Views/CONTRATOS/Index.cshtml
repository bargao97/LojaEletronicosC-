@model IEnumerable<WebApplication.Models.CONTRATO>

@{
    ViewBag.Title = "Metas";
}

<h2>Index</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.NOME_CLIENTE)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.META)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.NOME_CLIENTE)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.META)
        </td>
        <td>
            @Html.ActionLink("Editar", "Edit", new { id=item.ID_CONTRATO }) |
            @Html.ActionLink("Detalhes", "Details", new { id=item.ID_CONTRATO }) |
        </td>
    </tr>
}

</table>
